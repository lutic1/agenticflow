      {
        "id": 21,
        "title": "Error Budget - Overall",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 56,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "(1 - ((1 - avg(up{job=\"agent-booster\"})) * 100)) * 100",
            "legendFormat": "Agent Booster (Budget: 99.95%)",
            "refId": "A"
          },
          {
            "expr": "(1 - ((1 - avg(up{job=\"reasoningbank\"})) * 100)) * 100",
            "legendFormat": "ReasoningBank (Budget: 99.9%)",
            "refId": "B"
          },
          {
            "expr": "(1 - ((1 - avg(up{job=\"router\"})) * 100)) * 100",
            "legendFormat": "Router (Budget: 99.95%)",
            "refId": "C"
          },
          {
            "expr": "(1 - ((1 - avg(up{job=\"quic\"})) * 100)) * 100",
            "legendFormat": "QUIC (Budget: 99.99%)",
            "refId": "D"
          },
          {
            "expr": "(1 - ((1 - avg(up{job=\"agentdb\"})) * 100)) * 100",
