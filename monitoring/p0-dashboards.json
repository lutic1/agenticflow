{
  "dashboard": {
    "title": "Agentic Flow - P0 Features Monitoring",
    "tags": ["p0", "sre", "production"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 24,
          "h": 4
        },
        "targets": [
          {
            "expr": "min(up{job=~\"agent-booster|reasoningbank|router|quic|agentdb|swarm-coordinator\"})",
            "legendFormat": "Overall Health",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.9995, "color": "yellow"},
                {"value": 0.9999, "color": "green"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "1", "text": "Healthy"},
              {"type": "value", "value": "0", "text": "Down"}
            ],
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 2,
        "title": "Agent Booster - Operation Latency",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(agent_booster_operation_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(agent_booster_operation_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 (SLO: 5ms)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(agent_booster_operation_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 (SLO: 10ms)",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency"
          }
        ],
        "alert": {
          "name": "Agent Booster - High Latency",
          "conditions": [
            {
              "evaluator": {
                "params": [0.005],
                "type": "gt"
              },
              "query": {
                "params": ["B", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "noDataState": "no_data",
          "notifications": [
            {"uid": "pagerduty-p0"}
          ]
        }
      },
      {
        "id": 3,
        "title": "Agent Booster - Throughput",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(agent_booster_operations_total[1m])",
            "legendFormat": "Operations/sec (SLO: ≥1000)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Throughput"
          }
        ],
        "alert": {
          "name": "Agent Booster - Low Throughput",
          "conditions": [
            {
              "evaluator": {
                "params": [1000],
                "type": "lt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "notifications": [
            {"uid": "slack-alerts"}
          ]
        }
      },
      {
        "id": 4,
        "title": "Agent Booster - Success Rate",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(agent_booster_operations_total{status=\"success\"}[5m])) / sum(rate(agent_booster_operations_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.995, "color": "yellow"},
                {"value": 0.999, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "decimals": 3
          }
        }
      },
      {
        "id": 5,
        "title": "ReasoningBank - Query Latency",
        "type": "graph",
        "gridPos": {
          "x": 6,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(reasoningbank_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(reasoningbank_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 (SLO: 50ms)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(reasoningbank_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 (SLO: 100ms)",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency"
          }
        ],
        "alert": {
          "name": "ReasoningBank - High Query Latency",
          "conditions": [
            {
              "evaluator": {
                "params": [0.05],
                "type": "gt"
              },
              "query": {
                "params": ["B", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "notifications": [
            {"uid": "pagerduty-p1"}
          ]
        }
      },
      {
        "id": 6,
        "title": "ReasoningBank - Search Accuracy",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 12,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "avg(reasoningbank_search_accuracy_score)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.90, "color": "yellow"},
                {"value": 0.95, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0
          }
        }
      },
      {
        "id": 7,
        "title": "Multi-Model Router - Request Success Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(router_requests_total{status=\"success\"}[5m])) / sum(rate(router_requests_total[5m]))",
            "legendFormat": "Success Rate (SLO: ≥99.5%)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Success Rate",
            "max": 1,
            "min": 0.99
          }
        ],
        "alert": {
          "name": "Router - Low Success Rate",
          "conditions": [
            {
              "evaluator": {
                "params": [0.995],
                "type": "lt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "notifications": [
            {"uid": "pagerduty-p1"}
          ]
        }
      },
      {
        "id": 8,
        "title": "Multi-Model Router - Cost Savings",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 20,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "(router_baseline_cost - router_actual_cost) / router_baseline_cost",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.70, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "decimals": 1
          }
        }
      },
      {
        "id": 9,
        "title": "Multi-Model Router - Daily Spend",
        "type": "graph",
        "gridPos": {
          "x": 18,
          "y": 20,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(router_cost_usd[1d]))",
            "legendFormat": "Daily Cost",
            "refId": "A"
          },
          {
            "expr": "5",
            "legendFormat": "Budget Alert ($150/month = $5/day)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "currencyUSD",
            "label": "Cost"
          }
        ],
        "alert": {
          "name": "Router - Cost Budget Exceeded",
          "conditions": [
            {
              "evaluator": {
                "params": [6.67],
                "type": "gt"
              },
              "query": {
                "params": ["A", "1d", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "6h",
          "handler": 1,
          "notifications": [
            {"uid": "slack-alerts"}
          ]
        }
      },
      {
        "id": 10,
        "title": "QUIC Transport - Message Latency (RTT)",
