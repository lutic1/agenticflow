    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(deployment_timestamp[1m]) > 0",
          "iconColor": "blue",
          "tagKeys": "service,version"
        },
        {
          "name": "Incidents",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "incident_active == 1",
          "iconColor": "red",
          "tagKeys": "severity,component"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "custom",
          "options": [
            {"text": "Production", "value": "prod"},
            {"text": "Staging", "value": "staging"},
            {"text": "Development", "value": "dev"}
          ],
          "current": {
            "text": "Production",
            "value": "prod"
          }
        }
      ]
    }
  }
}
