{
  "dashboard": {
    "title": "Agentic Flow - P1 Features Monitoring",
    "tags": ["p1", "sre", "slide-designer", "features"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "P1 Overall System Health",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 24,
          "h": 4
        },
        "targets": [
          {
            "expr": "avg(p1_batch_health_score)",
            "legendFormat": "Overall P1 Health",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 98, "color": "green"}
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        }
      },
      {
        "id": 2,
        "title": "Feature Flag Status",
        "type": "table",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 24,
          "h": 6
        },
        "targets": [
          {
            "expr": "p1_feature_flag_rollout_percentage",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "flag": "Feature Flag",
                "Value": "Rollout %"
              }
            }
          }
        ]
      },
      {
        "id": 3,
        "title": "Batch 1: Quick Wins - Health Score",
        "type": "gauge",
        "gridPos": {
          "x": 0,
          "y": 10,
          "w": 6,
          "h": 6
        },
        "targets": [
          {
            "expr": "p1_batch_health_score{batch=\"batch1\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 98, "color": "green"}
              ]
            },
            "unit": "percent",
            "max": 100,
            "min": 0
          }
        }
      },
      {
        "id": 4,
        "title": "Batch 2: Content Enhancement - Health Score",
