      {
        "id": 8,
        "title": "P0 Performance Impact (P1 Enabled)",
        "type": "graph",
        "gridPos": {
          "x": 6,
          "y": 16,
          "w": 18,
          "h": 8
        },
        "targets": [
          {
            "expr": "(p0_with_p1_latency / p0_baseline_latency) * 100",
            "legendFormat": "{{p0_component}} - Latency Impact %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Performance Impact",
            "min": 90,
            "max": 120
          }
        ],
        "alert": {
          "name": "P1 Degrading P0 Performance",
          "conditions": [
            {
              "evaluator": {
                "params": [110],
                "type": "gt"
              },
              "query": {
                "params": ["A", "10m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "noDataState": "no_data",
          "notifications": [
            {"uid": "pagerduty-p1"}
          ]
        }
      },
      {
