            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(p1_feature_request_duration_seconds_bucket{feature=\"icon_library\",operation=\"search\"}[5m]))",
            "legendFormat": "p95 (SLO: 10ms)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ms",
            "label": "Latency"
          }
        ]
      },
