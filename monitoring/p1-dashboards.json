    "annotations": {
      "list": [
        {
          "name": "P1 Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(p1_deployment_timestamp[1m]) > 0",
          "iconColor": "blue",
          "tagKeys": "feature,batch,version"
        },
        {
          "name": "Feature Flag Changes",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(p1_feature_flag_rollout_percentage[1m]) > 0",
          "iconColor": "green",
          "tagKeys": "flag,action"
